<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2">
  <class name="Development.Dal.Error.Model.ErrorDao,Development.Dal" table="Error" lazy="true">
    <id name="Id" column="ID" type="int">
      <generator class="native"/>
    </id>
    <property type="string"  name="Message" column="Message"/>
    <property type="string" name="StackTrace" column="StackTrace"/>
    <property type="DateTime"  name="DateCreated" column="DateCreated"/>
  </class>
</hibernate-mapping>
